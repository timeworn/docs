{
    "name": "@timeworn/fumadocs-page-metadata",
    "version": "0.0.1",
    "description": "Fumadocs plugin for getting page metadata",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist",
        "types:check": "tsc --noEmit"
    },
    "dependencies": {
        "tinyexec": "^1.0.2"
    },
    "devDependencies": {
        "@types/node": "^25.2.3",
        "typescript": "^5.9.3"
    }
}